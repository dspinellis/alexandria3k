digraph DatabaseSchema {
  node [shape=plaintext]
  
  // Tables
  us_patent [label=<<table border="0" cellborder="1" cellspacing="0">
    <tr><td bgcolor="#DDEEFF" colspan="2"><b>us_patent</b></td></tr>
